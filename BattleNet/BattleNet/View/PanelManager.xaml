<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BattleNet.View"
             xmlns:Utils="clr-namespace:BattleNet.Utils" xmlns:BattleNet="clr-namespace:BattleNet" x:Class="BattleNet.View.PanelManager"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Utils:GameContent x:Name="GameContent" Visibility="Hidden"></Utils:GameContent>

        <BattleNet:US_AllGames x:Name="AllGames" Visibility="Hidden"></BattleNet:US_AllGames>

        <local:ShopPanel x:Name="ShopPanel" Visibility="Hidden"></local:ShopPanel>

    </Grid>
</UserControl>
